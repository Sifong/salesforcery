{
    "name": "stratease/salesforcery",
    "description": "A PHP Salesforce ORM, inspired by Eloquent's API.",
    "keywords": ["salesforce","php","orm","rest","database","model","client"],
    "type": "library",
    "license":"Apache-2.0",
    "homepage":"https://github.com/stratease/salesforcery",
    "authors": [
        {
            "name": "Edwin Daniels",
            "homepage": "https://github.com/stratease"
        }
    ],
    "require": {
        "php": ">=8.2",
        "guzzlehttp/guzzle": "7.9.2"
    },
    "require-dev": {
        "phpunit/phpunit": "11.3.1"
    },
    "autoload": {
        "psr-4": {
            "Stratease\\Salesforcery\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stratease\\Salesforcery\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
